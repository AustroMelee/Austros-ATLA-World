{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "node",
    "outDir": "./dist-scripts",
    "rootDir": "./scripts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false // <-- This is the crucial part. Ensure it's false or the line is removed.
  },
  "include": ["scripts/**/*.ts", "scripts/**/*.mts"],
  "exclude": ["node_modules"]
}