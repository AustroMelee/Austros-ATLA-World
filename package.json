{
  "name": "react-vanilla-extract-baseline",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix --max-warnings=0",
    "type-check": "tsc --noEmit",
    "parse:characters": "node scripts/parse-character-md.mjs",
    "validate:data": "node scripts/validate-data.mjs",
    "enrich:data": "node scripts/enrich-data.mjs",
    "build:index": "node scripts/build-index.mjs",
    "build:data": "npm run parse:characters && npm run enrich:data && npm run build:index",
    "lint:md": "markdownlint --config .markdownlint.json \"**/*.md\"",
    "build:tailwind": "tailwindcss -i ./src/styles/pure-tailwind.css -o ./src/styles/generated-tailwind.css --watch"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "markdownlint-cli": "^0.39.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^1.0.1"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "flexsearch": "^0.7.43",
    "gray-matter": "^4.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "remark-gfm": "^4.0.1",
    "zod": "^4.0.5"
  }
}
